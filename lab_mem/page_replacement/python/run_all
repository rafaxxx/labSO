#/bin/bash

# This script exercises the real workload
npages=1500000
clock=200

for trace in trace.1 trace.2; do
  #for alg in fifo nru aging second-chance
  for alg in fifo second_chance; do
    echo "=== ALGORITMO $alg ==="
    for nframes in 4 8 16 32; do
	    mkdir -p output/$trace
      echo "Simulacao para npages = $npages nframes = $nframes clock = $clock"
	    python memsym.py $npages $nframes $alg $clock < load/$trace.in > output/$trace/$trace.$alg.$nframes.out
    done
  done
done
